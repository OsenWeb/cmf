<div>
    {% if service('bw_user.social').vkontakte %}
        <a href="{{ path('user_vkontakte_sign_in') }}" title="Авторизироваться через аккаунт ВКонтакте" rel="nofollow">
            <img src="{{ asset('images/social/vkontakte/sign-in/sign-in-with-vkontakte-x44.png') }}" alt="Sign in with VKontakte" width="177" height="44">
        </a>
    {% endif %}
    {% if service('bw_user.social').facebook %}
        <a href="{{ path('user_facebook_sign_in') }}" title="Авторизироваться через Facebook аккаунт" rel="nofollow">
